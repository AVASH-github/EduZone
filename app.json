{
  "expo": {
    "name": "EduZone",
    "slug": "EduZone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-android.png",
    "scheme": "eduzone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.avash1017.EduZone",
      "googleServicesFile" : "./android/app/google-services.json"

    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#4842eb"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4c1ae7e7-76a7-4183-b08d-7c2b9627827f"
      }
    }
  }
}
